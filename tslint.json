{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "array-type": [
      true,
      "generic"
    ],
    "no-namespace": false,
    "only-arrow-functions": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "space-before-function-paren": false,
    "trailing-comma": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}